{
  "$schema": "https://infection.github.io/schema.json",
  "source": {
    "directories": ["src"],
    "excludes": ["src/Events"]
  },
  "logs": {
    "text": "infection-log.txt"
  },
  "mutators": {
    "@default": true,
    "UnwrapArray": true
  },
  "phpUnit": {
    "configDir": "."
  },
  "minMsi": 55,
  "minCoveredMsi": 65,
  "timeout": 10
}
